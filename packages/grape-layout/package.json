{
    "name": "@idealjs/grape-layout",
    "version": "1.7.3",
    "main": "dist/index.js",
    "types": "dist/components/index.d.ts",
    "dependencies": {
        "@idealjs/portal-window": "^1.0.0",
        "clsx": "^1.1.1",
        "jss": "^10.6.0",
        "jss-preset-default": "^10.6.0"
    },
    "devDependencies": {
        "@idealjs/layout-manager": "^1.7.3",
        "@idealjs/sns": "^1.7.3"
    },
    "peerDependencies": {
        "@idealjs/layout-manager": "^1.7.0",
        "@idealjs/sns": "^1.7.3"
    },
    "scripts": {
        "start": "react-scripts start",
        "lint": "eslint src/**/*.{tsx,ts} --fix",
        "build:webpack": "webpack -c config/webpack.config.js",
        "prepublishOnly": "rm -rf dist&yarn build:webpack"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "plugins": [
            "simple-import-sort"
        ],
        "rules": {
            "simple-import-sort/imports": "warn",
            "simple-import-sort/exports": "warn",
            "react-hooks/exhaustive-deps": [
                1,
                {
                    "enableDangerousAutofixThisMayCauseInfiniteLoops": true
                }
            ]
        }
    },
    "eslintIgnore": [
        "dist"
    ],
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
